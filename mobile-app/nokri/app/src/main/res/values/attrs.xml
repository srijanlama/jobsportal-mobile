<?xml version="1.0" encoding="utf-8"?>
<resources>


        <declare-styleable name="MaterialProgressBar">
            <attr name="progressColour" format="color" />
            <attr name="secondaryProgressColour" format="color" />
            <attr name="backgroundColour" format="color" />
            <attr name="duration" format="integer" />
        </declare-styleable>


    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="ColorPickerView">
        <attr name="colorViewWidth" format="dimension" />
        <attr name="colorViewHeight" format="dimension" />
        <attr name="colorViewMarginLeft" format="dimension" />
        <attr name="colorViewMarginRight" format="dimension" />
        <attr name="colorViewCheckedType" format="enum">
            <enum name="dot" value="0" />
            <enum name="checkMark" value="1" />
        </attr>
        <attr name="colors" format="reference" />
    </declare-styleable>

    <declare-styleable name="are">
        <attr name="expandMode" format="enum">
            <enum name="FULL" value="0" />
            <enum name="MIN" value="1" />
        </attr>
        <attr name="hideToolbar" format="boolean" />
        <attr name="toolbarAlignment" format="enum">
            <enum name="BOTTOM" value="0" />
            <enum name="TOP" value="1" />
        </attr>
        <attr name="useEmoji" format="boolean" />
    </declare-styleable>


</resources>